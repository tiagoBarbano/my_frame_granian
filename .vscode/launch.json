    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main_fastapi:app",
                "--reload"
            ],
            "jinja": true
        },
        {
            "name": "Python Debugger: Granian",
            "type": "debugpy",
            "request": "launch",
            "module": "granian",
            "args": [
                "--interface",
                "asginl",
                "main_granian:app"
            ],
            "jinja": true
        }
    ]

}